<div class="card">
    <div class="card-body">
        <h4 class="card-title">@AppResource.lbCategorys</h4>
        <button type="button" class="btn btn-primary" onclick="OpenModel()">@AppResource.lbCreateNew</button>
        <br />
        <br />
        <table id="tblData" class="table table-bordered table-striped ">
            <thead>
                <tr>
                    <th>@AppResource.lbSerial</th>
                    <th>@AppResource.lbName</th>
                    <th>@AppResource.lbDescription</th>
                    <th>...</th>
                </tr>
            </thead>

            <tfoot>
                <tr>
                    <th>@AppResource.lbSerial</th>
                    <th>@AppResource.lbName</th>
                    <th>@AppResource.lbDescription</th>
                    <th>...</th>
                </tr>
            </tfoot>
        </table>
    </div>
</div>
<partial name="CategorysAction.cshtml"></partial>
@section Scripts
{
    <script src="~/pages/foods/categorys.js"></script>
    <script src="~/pages/foods/categorysaction.js"></script> 
}