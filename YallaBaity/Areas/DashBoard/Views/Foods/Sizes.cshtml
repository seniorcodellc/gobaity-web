
<div class="card">
    <div class="card-body">
        <h4 class="card-title">@AppResource.lbSizes</h4>

        <button type="button" class="btn btn-primary" onclick="OpenModel()">@AppResource.lbCreateNew</button>
        <br />
        <br />
        <table id="tblData" class="table table-bordered table-striped ">
            <thead>
                <tr>
                    <th>@AppResource.lbSerial</th>
                    <th>@AppResource.lbName</th>
                    <th>...</th>
                </tr>
            </thead>

            <tfoot>
                <tr>
                    <th>@AppResource.lbSerial</th>
                    <th>@AppResource.lbName</th>
                    <th>...</th>
                </tr>
            </tfoot>
        </table>
    </div>
</div>

<partial name="SizesAction.cshtml"></partial>

@section Scripts
{
    <script src="~/pages/foods/sizes.js"></script>
    <script src="~/pages/foods/sizesaction.js"></script>
}

